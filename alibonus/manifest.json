{
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icons/favicon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/content.css"
      ],
      "js": [
        "js/lib/jquery-3.0.0.min.js",
        "js/content/content.js"
      ],
      "matches": [
        "*://*.aliexpress.com/*"
      ]
    },
    {
      "js": [
        "js/content/alibonus.js"
      ],
      "matches": [
        "*://*.alibonus.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "default_locale": "ru",
  "description": "__MSG_appDesc__",
  "homepage_url": "https://alibonus.com/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "options_page": "options.html",
  "permissions": [
    "cookies",
    "notifications",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.21.23",
  "web_accessible_resources": [
    "html/*",
    "js/*",
    "icons/*"
  ]
}
