  "browser_action": {
    "default_icon": {
      "16": "icons/ico-16.png",
      "19": "icons/ico-19.png",
      "20": "icons/ico-20.png",
      "32": "icons/ico-32.png",
      "38": "icons/ico-38.png",
      "40": "icons/ico-40.png"
    },
    "default_popup": "popup.html",
    "default_title": "Hello world!"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "preload.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": true,
      "js": [
        "postload.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "developer": {
    "name": "Dang Lucks Oft"
  },
